# Environment Variables Example
# Copy this file to .env.local and fill in your values

# App Configuration
NEXT_PUBLIC_APP_NAME=AbyssBuilds
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# Storage Configuration
NEXT_PUBLIC_STORAGE_PREFIX=abyssbuilds_

# Future: API Configuration (when backend is added)
# NEXT_PUBLIC_API_URL=https://api.example.com
# NEXT_PUBLIC_API_KEY=your_api_key_here

# Future: Authentication (when backend is added)
# NEXT_PUBLIC_AUTH_DOMAIN=auth.example.com
# NEXT_PUBLIC_AUTH_CLIENT_ID=your_client_id

# Future: Analytics (optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_SENTRY_DSN=https://xxx@sentry.io/xxx
